{
  "name": "geocoordinate",
  "version": "2.0.38",
  "author": "Christian Kaatz <christian.kaatz@here.com>",
  "description": "Classes to work with geo coordinates and bounding boxes",
  "repository": "git://github.com/chrkaatz/GeoCoordinate.git",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watchAll --coverage",
    "lint": "eslint lib/",
    "generate-docs": "jsdoc --configure .jsdoc.json --verbose"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-plugin-jest": "24.4.0",
    "jest": "27.2.1",
    "jsdoc": "4.0.5",
    "lodash": "4.17.21",
    "minami": "1.2.3",
    "prettier": "3.8.0",
    "random-seed": "0.3.0"
  },
  "engines": {
    "node": ">= 8"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.{js,json}",
      "!/node_modules/"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 90,
        "functions": 95,
        "lines": 95
      }
    }
  }
}
