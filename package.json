{
  "name": "geocoordinate",
  "version": "1.2.4",
  "author": "Christian Kaatz <christian.kaatz@here.com>",
  "description": "Classes to work with geo coordinates and bounding boxes",
  "repository": "git://github.com/chrkaatz/GeoCoordinate.git",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watchAll --coverage",
    "lint": "eslint lib/",
    "generate-docs": "jsdoc --configure .jsdoc.json --verbose"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-plugin-jest": "^22.17.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "lodash": "^4.17.15",
    "minami": "^1.2.3",
    "prettier": "^1.18.2",
    "random-seed": "^0.3.0"
  },
  "engines": {
    "node": ">= 8"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.{js,json}",
      "!/node_modules/"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 90,
        "functions": 95,
        "lines": 95
      }
    }
  }
}
